        <activity android:name="com.infix.studentmanagement.activity.TransportActivity"
            android:parentActivityName="com.infix.studentmanagement.activity.HomeActivity"/>
        <activity android:name="com.infix.studentmanagement.activity.DormitoryActivity"
            android:parentActivityName="com.infix.studentmanagement.activity.HomeActivity"/>
        <activity android:name="com.infix.studentmanagement.activity.IssuedBookActivity" />
