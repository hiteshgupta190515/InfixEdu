        <activity android:name="com.infix.studentmanagement.activity.StudentTeacherActivity" />
        <activity android:name="com.infix.studentmanagement.activity.StudentSubjectActivity" />
        <activity
            android:name="com.infix.studentmanagement.activity.NoticeActivity"
            android:parentActivityName="com.infix.studentmanagement.activity.HomeActivity" />

        <uses-library
