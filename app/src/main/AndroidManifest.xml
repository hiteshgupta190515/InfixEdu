        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.infix.studentmanagement.activity.ChildListActivity"
            android:parentActivityName="com.infix.studentmanagement.activity.HomeActivity"/>
        <activity
